
<p>
  <strong>
      <%= @user.name + "のページ" %>
  </strong>
</p>

<div>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Deadline</th>
      <th>State</th>
      <th>Importance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.tasks.each do |task| %>
      <tr>
        <td><%= task.title %></td>
        <td><%= task.content %></td>
        <td><%= task.end_on %></td>
        <td><%= task.status %></td>
        <td><%= task.importance %></td>

      </tr>
    <% end %>

  </tbody>
</table>
</div>


<%= link_to 'userを編集する',edit_admin_user_path(@user) %>
<br>
<%= link_to 'userを削除する',admin_user_path(@user), method: :delete%>
<br>
<%= link_to 'userを作成する', new_admin_user_path %>
